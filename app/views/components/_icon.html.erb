<% elsif name === "chevron-bottom" && size === "small" && style === "outline" %>
  <svg width="16" height="16" viewBox="0 0 16 16" fill="none">
    <path
      d="M4 6L8 10L12 6"
      stroke="<%= color %>"
      stroke-linecap="round"
      stroke-linejoin="round"
    />
  </svg>
<% elsif name === "chevron-right" && size === "small" && style === "outline" %>
  <svg width="16" height="16" viewBox="0 0 16 16" fill="none">
    <path
      d="M6 12L10 8L6 4"
      stroke="<%= color %>"
      stroke-linecap="round"
      stroke-linejoin="round"
    />
  </svg>
<% elsif name === "chevron-left" && size === "small" && style === "outline" %>
  <svg width="16" height="16" viewBox="0 0 16 16" fill="none">
    <path
      d="M10 4L6 8L10 12"
      stroke="<%= color %>"
      stroke-linecap="round"
      stroke-linejoin="round"
    />
  </svg>
<% end %>